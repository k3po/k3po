connect tcp://localhost:1080
connected

## Write the request bytes for connecting with no-authentication
# version: SOCKS v5
write [0x05]

# number of method: 1
write [0x01]

# method: no authentication required
write [0x00]


## Read the response, expect success
# version: SOCKS v5
read [0x05]

# selected method: no authentication required
read [0x00]

## Write BIND request
# version: SOCKS v5
write [0x05]

# BIND command
write [0x02]

# RSV field
write [0x00]

# ATYP with URI
write [0xFE]

# DST.ADDR with URI
# URI length
write [0x00 0x00 0x00 0x0A]

# URI
write "pipe://foo" [0x00]

# DST.PORT - invalid port
write [0x00 0x01]

## Read BIND response
# version: SOCKS v5
read [0x05]

# Connection refused response
read [0x05]

# RSV field
read [0x00]

# ATYP with URI
read [0xFE]

# BND.ADDR with URI
# URI length
read [0x00 0x00 0x00 0x0A]

# URI
read "pipe://foo" [0x00]

# BND.PORT
read [0x00 0x01]

close
closed
