property challenge "Application-Token realm=\"demo\", saml-authn-request=\"$validAuthNRequest\""

connect ${location}
connected

write method "GET"
write close
read status "401" "Unauthorized"
read header "WWW-Authenticate" ${challenge}
read notify FIRST_REQUEST_COMPLETE

connect await FIRST_REQUEST_COMPLETE
connect ${location}
connected
write method "GET"
write header "Authorization" ${validAuthNResponse}
write close
read status "200" "OK"
