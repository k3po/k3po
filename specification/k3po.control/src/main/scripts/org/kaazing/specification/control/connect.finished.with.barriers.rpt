connect tcp://localhost:12345
connected

write "PREPARE\n"
write "version:2.0\n"
write "name:org/kaazing/specification/control/assets/server\n"
write "name:org/kaazing/specification/control/assets/client.with.barriers\n"
write "\n"

read "PREPARED\n"
read "barrier:AWAITING_BARRIER\n"
read "barrier:NOTIFYING_BARRIER\n"
read "content-length:233\n"
read "\n"
read "# server\n"
read "accept tcp://localhost:8000\n"
read "accepted\n"
read "connected\n"
read "read \"echo\"\n"
read "write \"echo\"\n"
read "close\n"
read "closed\n"
read "# client\n"
read "connect tcp://localhost:8000\n"
read "connected\n"
read "read notify NOTIFYING_BARRIER\n"
read "write await AWAITING_BARRIER\n"
read "write \"echo\"\n"
read "read \"echo\"\n"
read "closed\n"

write "START\n"
write "\n"

read "STARTED\n"
read "\n"

write "AWAIT\n"
write "barrier:NOTIFYING_BARRIER\n"
write "\n"

read "NOTIFIED\n"
read "barrier:NOTIFYING_BARRIER\n"
read "\n"

write "NOTIFY\n"
write "barrier:AWAITING_BARRIER\n"
write "\n"

read "NOTIFIED\n"
read "barrier:AWAITING_BARRIER\n"
read "\n"

read "FINISHED\n"
read "content-length:233\n"
read "\n"
read "# server\n"
read "accept tcp://localhost:8000\n"
read "accepted\n"
read "connected\n"
read "read \"echo\"\n"
read "write \"echo\"\n"
read "close\n"
read "closed\n"
read "# client\n"
read "connect tcp://localhost:8000\n"
read "connected\n"
read "read notify NOTIFYING_BARRIER\n"
read "write await AWAITING_BARRIER\n"
read "write \"echo\"\n"
read "read \"echo\"\n"
read "closed\n"

write "DISPOSE\n"
write "\n"

read "DISPOSED\n"
read "\n"

close
